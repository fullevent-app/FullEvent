{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "cd ../.. && pnpm build:api",
    "installCommand": "cd ../.. && pnpm install",
    "framework": null,
    "functions": {
        "api/**/*.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}